                <li><apex:commandButton id="btnReset" action="{!ClearSearch}" value="{!$Label.Finder_Search_Reset}" styleClass="btn search-btn previous"/></li>
              </ul>
                  <!--<button class="btn btn-default" type="button">Go!</button>-->
              </div>
            </div>
        </section>
