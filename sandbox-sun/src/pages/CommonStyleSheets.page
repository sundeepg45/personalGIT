<apex:page contentType="text/css" showHeader="false" cache="false">
  body{
    font-family:Arial,Helvetica,sans-serif !important;
    font-size: 12px !important;
    color: #222 !important;
 }
  
  .RH_bg-fade{
    
     display: none;
     width: 100%;
     height: 100%;
     position: fixed;
     z-index: 10000;
     background: #000;
     opacity: 0.5;
     filter:alpha(opacity=50) !important;
     top: 0;
     left: 0;
 }
 
 .RH_modal-window{
    width: 800px;
    height: auto;
    overflow: hidden;
    position: absolute;
    z-index: 10001;
    left: 50%;
    margin-left: -400px;
    top: 80px;
    border-radius: 5px;  
    background: #fff;   
    box-shadow: 0 5px 20px #333333;
    -ms-filter: "progid:DXImageTransform.Microsoft.Shadow(Strength=5, Direction=130, Color='#cccccc')"; 
    display: none;
 }
 
 .RH_modal-head{
    height: 38px;
    background : #ECF0F1;
 }
 
 .RH_left{
     float: left;
 }
 .RH_right{
     float: right;
 }
 
 #RH_groupMember-header{  
    padding: 10px 0 0 10px;
    font-weight: bold;
    color: #222;   
 }
 
 .RH_modal-content{
    padding: 10px;
    max-height: 400px;
    overflow-y: auto;
 }
 
 .RH_modal-btm{
    padding: 10px;
    height: auto;
    overflow: hidden;
    border-top: 1px solid #e1e7e9;
    background: #ecf0f1;
 }
 
 .RH_Header{
    padding:10px;
    border:1px solid #e0e0e0;
    font-weight:bold;
    font-size:15px;
    font-family:arial;
    border-radius:5px;
    color:#fff;
    background:#EBCF5E;
 }
 
 .modal-group-header{
     padding: 10px 10px;
     font-weight : bold;
 }
 
 .RH_Opp_Detail{
     margin-bottom: 30px;
 }
 
 .RH_Opp_Detail_Width{
     width: 50%;
 }
 
 .RH_Opp_Detail_Header{
     font-family:arial;
     padding-top:5px;
     margin-left: 30%;
     font-weight:bold;
 }
 
 .RH_Opp_Margin{
     margin-left:30%;
 }
 
 .RH_Opp_Field_Label{
     font-family:arial;
     font-weight:bold;
     margin-right:10px;
 }
 
 .RH_Opp_Field_Value{
     font-family:arial;
 }
 
 .RH_Opp_Tbl_Data{
     padding-top:10px;
 }
 
 .RH_OrderDetails_Header_Padding{
    background: #f8f8f8;      
 }
 
 .RH_fullwidth{
     width:100%;
 }
 
 .RH_ExpandCollapseMenu{
     padding-left:30px;
     cursor:pointer;
 }
 
 .RH_PointerCursor{
    cursor:pointer;
 }
 
 .RH_MenuTitle{
     padding:10px;
     background-color: #d3d3d3;
     border-radius:5px;
     font-size:12px;
     font-family:arial;
     font-weight:bold;
     cursor:pointer;
 }
 
 .RH_firsrMenuTitle{
     margin-top:10px;
 }
 
 .RH_Hide{
     display:none;
 }
 
 .RH_show{
     display:block;
 }
 
 .RH_contentOutlineBorder{
     border:1px solid #d3d3d3;
     margin : 10px 0px 0px 50px;
     width:1100px;
     padding:10px;
 }
 
 .RH_inputFieldWidth{
     width:320px;
     height:30px;
     padding-left:5px;
 }
 
 .RH_inputFieldMargin{
     margin-top:10px;
 }
 
 .RH_secondInputMargin{
     margin-left:100px
 }
 
 .RH_submitButton{
     margin-left:33%;
     margin-top:20px;
     cursor : pointer;
 }
 
 .RH_Button_Padding{
     padding : 3px 5px;
 }
 
 .RH_closePopup,RH_createAddress{
     float:left;
 }
 
 #map_canvas {
     width: 600px;
     height: 400px;
 }
 
 .inputFieldWidth{
     width : 375px;
 }
 
 .hideThis{
     display:none;
 }
 
 .buttonClass{
     padding : 5px 10px;
     cursor : pointer;
 }
 
 .RH_StyleSection{
    background: #f8f8f8;
    margin-top: 20px;
    border-left: 1px solid #E8e8e8;
    border-bottom: 1px solid #E8E8E8;
    border-right: 1px solid #E8E8E8;
 }
</apex:page>