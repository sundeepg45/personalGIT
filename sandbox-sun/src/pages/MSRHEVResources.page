<apex:page controller="MSController" showHeader="false" sidebar="false" standardStylesheets="false" action="{!initWithContent}">
	<apex:include pageName="RHEV_include_header"/>
	<body id="sixth" class="ms dark">
	<div id="wrapper">
		<div id="page">
			<div class="container_12 clearfix">
				<div id="header" class="grid_12">
					<div id="sm-logo"><a href="http://www.redhat.com">Red Hat</a></div><!-- /#sm-logo -->
					<div id="header-links">
					<ul style="margin-right: 8px;">
						<li class="first" style=""></li>
					</ul>
						<div class="clear"></div>
						<a target="_blank" class="header-button" href="http://www.redhat.com">redhat.com</a><!-- /.header-button -->
					</div><!-- /#header-links -->
					<div id="nav-band">
					  <div id="primary-nav">
					  	<apex:include pageName="MSRHEV_include_nav"/>
					  </div><!-- /#primary-nav -->
					</div><!-- /#nav-band -->
					  	<apex:include pageName="RHEV_include_promo"/>
				</div><!-- /#header -->
				<div class="clear"></div>

				<div id="main">
<div class="grid_12">
	
	<div class="grid_9 alpha">
<h1 class="boxed">Resources</h1>
<p></p>
		<p>Your source for the latest information about Red Hat&reg; Enterprise Virtualization.</p>
<div class="grid_4 alpha">
    <c:MSSection pageController="{!this}" sectionKey="Datasheets">
		<h2>Datasheets</h2>
		<dl class="looks">
        	<c:MSSectionList pageController="{!this}" sectionKey="Datasheets"/>
		</dl>
    </c:MSSection>

	<div class="clear"></div>

    <c:MSSection pageController="{!this}" sectionKey="Pricing">
		<h2>Pricing</h2>
		<dl class="looks">
        	<c:MSSectionList pageController="{!this}" sectionKey="Pricing"/>
		</dl>
    </c:MSSection>

	<div class="clear"></div>

    <c:MSSection pageController="{!this}" sectionKey="Features">
 		<h2>Features</h2>
		<dl class="looks">
        	<c:MSSectionList pageController="{!this}" sectionKey="Features"/>
		</dl>
    </c:MSSection>

	<div class="clear"></div>
</div><!-- grid_4 -->

<div class="grid_4 omega">
    <c:MSSection pageController="{!this}" sectionKey="Competitive">
 		<h2>Competitive</h2>
		<dl class="looks">
        	<c:MSSectionList pageController="{!this}" sectionKey="Competitive"/>
		</dl>
    </c:MSSection>

	<div class="clear"></div>

    <c:MSSection pageController="{!this}" sectionKey="Services">
 		<h2>Services</h2>
		<dl class="looks">
        	<c:MSSectionList pageController="{!this}" sectionKey="Services"/>
		</dl>
    </c:MSSection>

</div><!-- grid_4 -->
	<div class="clear"></div>
	</div><!-- /.grid_9 -->	
	<div class="grid_3 omega">
	  	<apex:include pageName="RHEV_include_contact"/>
	</div><!-- /.grid_3 -->
	<div class="clear"></div>

		
		</div><!-- /.grid_12 -->
	</div><!-- /#main -->
	<div class="clear"></div>
				
<apex:include pageName="RHEV_include_footer"/>

</div>
</div>
</div>
</body>

</apex:page>