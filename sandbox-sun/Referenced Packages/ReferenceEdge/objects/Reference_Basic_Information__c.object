    <validationRules>
        <fullName>VR_Checkfor_IsReferenceable</fullName>
        <active>true</active>
        <description>You must select a &apos;Reference Status&apos; if &apos;Reference Program Member&apos; checkbox is selected.</description>
        <errorConditionFormula>AND(Is_Referenceable__c = true, ISPICKVAL(Referenceability_Status__c, &apos;&apos;))</errorConditionFormula>
        <errorMessage>You must select a &apos;Reference Status&apos; if &apos;Reference Program Member&apos; checkbox is selected.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VR_Checkfor_ReferenceabilityStatus</fullName>
        <active>true</active>
        <description>You must select &apos;Reference Program Member&apos; checkbox if &apos;Reference Status&apos; is &apos;Active&apos; or &apos;Caution&apos;.</description>
        <errorConditionFormula>AND(
OR(ISPICKVAL(Referenceability_Status__c, &apos;Active&apos;), ISPICKVAL(Referenceability_Status__c, &apos;Caution&apos;)) ,
Is_Referenceable__c = false
)</errorConditionFormula>
        <errorMessage>You must select &apos;Reference Program Member&apos; checkbox if &apos;Reference Status&apos; is &apos;Active&apos; or &apos;Caution&apos;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VR_Checkfor_UseTimePeriod</fullName>
        <active>true</active>
        <description>If &apos;Time Period&apos; is set then &apos;Uses Per Time Period&apos; MUST also be set.</description>
        <errorConditionFormula>AND(NOT(ISNULL(RUL_Period__c)),
ISNULL(RUL_Activities_will_do__c))</errorConditionFormula>
        <errorMessage>If &apos;Time Period&apos; is set then &apos;Uses Per Time Period&apos; MUST also be set.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VR_Checkfor_UsesPerTimePeriod</fullName>
        <active>true</active>
        <description>If &apos;Uses Per Time Period&apos; is set then &apos;Time Period&apos; MUST also be set.</description>
        <errorConditionFormula>AND(NOT(ISNULL(RUL_Activities_will_do__c)),
ISNULL(RUL_Period__c))</errorConditionFormula>
        <errorMessage>If &apos;Uses Per Time Period&apos; is set then &apos;Time Period&apos; MUST also be set.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VR_RUL_Activites</fullName>
        <active>true</active>
        <description>You must specify &apos;Uses per Time Period&apos; in the period from 1 to 99</description>
        <errorConditionFormula>( RUL_Activities_will_do__c  &gt; 99) || (RUL_Activities_will_do__c &lt; 1)</errorConditionFormula>
        <errorMessage>You must specify &apos;Uses per Time Period&apos; in the period from 1 to 99</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VR_RUL_Period</fullName>
        <active>true</active>
        <description>You must specify &apos;Time Period&apos; in the period from 1 to 365</description>
        <errorConditionFormula>(RUL_Period__c &gt; 365) ||(RUL_Period__c &lt; 1)</errorConditionFormula>
        <errorMessage>You must specify &apos;Time Period&apos; in the period from 1 to 365</errorMessage>
    </validationRules>
