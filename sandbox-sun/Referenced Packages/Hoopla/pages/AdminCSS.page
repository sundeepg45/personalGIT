.goal-grid {width:670px;padding: 8px 0 0 4px !important;overflow:auto}
p.goal-grid-label {font-weight: 600 !important; margin: 20px 0 7px 0 !important }
.goal-table {  border-collapse:collapse !important;border:none; }
.goal-table td.header {padding-left:10px;white-space:nowrap; color: #000000 !important; background-color: #DCDCDC !important;}
.goal-table td {background-color: #FFFFFF !important;border: 1px solid #C8C8C8; border-bottom: 1px solid #C8C8C8; color: #000000; }
.goal-table td.value-cell {min-width:100px !important;max-width:110px !important;width:100px !important;height:40px !important;}
img.across-arrow {position:relative;display:none;top:7px;left:-1px}
img.down-arrow {position:relative;display:none;top:-3px;left:40px}

.goal-table td.value-cell input {
    outline:none;
    transition: all 0.25s ease-in-out;
    -webkit-transition: all 0.25s ease-in-out;
    -moz-transition: all 0.25s ease-in-out;
    border-radius:3px;
    -webkit-border-radius:3px;
    -moz-border-radius:3px;
    border:1px solid rgba(0,0,0, 0.2);
}
.goal-table td.value-cell input.hilited {
    box-shadow: 0 0 5px rgba(0, 0, 255, 1);
    -webkit-box-shadow: 0 0 5px rgba(0, 0, 255, 1); 
    -moz-box-shadow: 0 0 5px rgba(0, 0, 255, 1);
    border:1px solid rgb(0,0,255);
}
 
